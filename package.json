{
  "name": "papereasy-build",
  "version": "0.2.0",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently -k \"pnpm:dev:renderer\" \"pnpm:dev:main\" \"pnpm:dev:preload\" \"pnpm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:main": "tsc -w -p tsconfig.main.json",
    "dev:preload": "tsc -w -p tsconfig.preload.json",
    "dev:electron": "wait-on tcp:5173 dist/main/main.js dist/preload/preload.js && electron .",
    "build": "pnpm build:main && pnpm build:preload && pnpm build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build:renderer": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "~6.1.0",
    "@dnd-kit/core": "^6.3.1",
    "antd": "^6.0.0",
    "dexie": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^8.2.0",
    "electron": "30",
    "typescript": "^5.5.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.0"
  }
}
